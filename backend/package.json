{
  "name": "telehealth-backend",
  "version": "1.0.0",
  "description": "Telehealth platform backend API",
  "main": "src/app.js",
  "type": "module",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --watch --detectOpenHandles",
    "db:migrate": "bash ../database/migrations/run_all_migrations.sh up",
    "db:rollback": "bash ../database/migrations/run_all_migrations.sh down",
    "db:status": "bash ../database/migrations/run_all_migrations.sh status",
    "db:seed": "node scripts/seed.js",
    "db:init": "npm run db:migrate && npm run db:seed",
    "db:reset": "npm run db:rollback && npm run db:migrate && npm run db:seed",
    "lint": "eslint src/",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@sendgrid/mail": "^8.1.5",
    "@supabase/supabase-js": "^2.38.4",
    "aws-sdk": "^2.1491.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "drizzle-kit": "^0.20.4",
    "drizzle-orm": "^0.29.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.10.1",
    "openai": "^5.20.2",
    "pg": "^8.11.3",
    "postgres": "^3.4.3",
    "sharp": "^0.32.6",
    "socket.io": "^4.7.4",
    "stripe": "^18.5.0",
    "twilio": "^4.23.0",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.8.10",
    "eslint": "^8.53.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
